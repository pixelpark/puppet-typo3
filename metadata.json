{
    "name": "puppet-typo3",
    "version": "0.5.2",
    "author": "puppetcommunity",
    "summary": "Compressed archive file download and extraction with native types/providers for Windows and Unix",
    "license": "Apache-2.0",
    "source": "https://github.com/puppet-community/puppet-archive",
    "project_page": "https://github.com/puppet-community/puppet-archive",
    "issues_url": "https://github.com/puppet-community/puppet-archive/issues",
        "dependencies": [
        { "name":"puppetlabs/stdlib", "version_requirement": ">= 4.9.0" },
        { "name":"puppet/archive",    "version_requirement": ">=0.5.1" }
    ],
    "operatingsystem_support": [
        { "operatingsystem": "RedHat",      "operatingsystemrelease": [ "4", "5", "6", "7" ] },
        { "operatingsystem": "CentOS",      "operatingsystemrelease": [ "4", "5", "6", "7" ] },
        { "operatingsystem": "OracleLinux", "operatingsystemrelease": [ "4", "5", "6", "7" ] }
    ],
    "requirements": [
        { "name": "pe",     "version_requirement": ">=3.2.0" },
        { "name": "puppet", "version_requirement": ">=2.7.20 <5.0.0" }
    ]
}
